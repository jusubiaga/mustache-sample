<div class="page-header">
  <h1>This is an example using templates</h1>
</div>

{{#panels}}
<div class="panel panel-default">
  <div class="panel-body">
	<h3 class="title">{{title}}</h3>
	<p class="desc">{{desc}}</p>

<ul class="list-group">
	{{#links}}
		<li class="list-group-item"><a href="{{url}}">{{title}}</a></li>
	{{/links}}
</ul>

  </div>
</div>

{{/panels}}